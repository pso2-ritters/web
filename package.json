{
	"name": "@pso2-ritters/web",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"prepare": "husky install",
		"build": "run-p build:*",
		"build:ts": "vue-tsc",
		"build:vite": "vite build",
		"dev": "vite",
		"format": "run-s format:* lint-fix",
		"format:prettier": "prettier --write .",
		"lint": "run-p lint:*",
		"lint:es": "eslint --cache .",
		"lint:dependency": "depcruise -v -- src/main.ts",
		"lint-fix": "run-p lint-fix:*",
		"lint-fix:es": "npm run lint:es -- --fix",
		"preview": "vite preview",
		"start": "npm run dev",
		"test": "run-p test:*",
		"test:vitest": "vitest run",
		"test-watch": "vitest watch"
	},
	"dependencies": {
		"vue": "^3.3.8"
	},
	"devDependencies": {
		"@types/ramda": "^0.29.8",
		"@typescript-eslint/eslint-plugin": "^6.10.0",
		"@typescript-eslint/parser": "^6.10.0",
		"@vitejs/plugin-vue": "^4.4.1",
		"dependency-cruiser": "^15.3.0",
		"eslint": "^8.53.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-import": "^2.29.0",
		"eslint-plugin-vue": "^9.18.1",
		"husky": "^8.0.0",
		"lint-staged": "^15.1.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.0.3",
		"ramda": "^0.29.1",
		"sort-package-json": "^2.6.0",
		"typescript": "^5.2.2",
		"unplugin-auto-import": "^0.16.7",
		"vite": "^4.5.0",
		"vitest": "^0.34.6",
		"vue-tsc": "^1.8.22"
	}
}
